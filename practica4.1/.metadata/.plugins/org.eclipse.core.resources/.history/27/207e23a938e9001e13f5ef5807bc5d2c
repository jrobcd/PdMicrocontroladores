#include "API_debounce.h"

void SystemClock_Config(void);

int main(void) {
    HAL_Init();
    SystemClock_Config();
    debounceFSM_init();

    while (1) {
        debounceFSM_update();

        if (readKey()) {
            if (HAL_GPIO_ReadPin(GPIOA, GPIO_PIN_1) == GPIO_PIN_SET) {
                HAL_Delay(500);
            } else {
                HAL_Delay(100);
            }
        }
    }
}

